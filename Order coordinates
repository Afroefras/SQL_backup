select 
    o.id as order_id,
    ST_X(ST_EndPoint(orr.path)) as lat,
    ST_Y(ST_EndPoint(orr.path)) as long
from orders_order o 
    join orders_orderroute orr on orr.order_id = o.id
    join catalog_storebranch sb on o.shopper_store_branch_id = sb.id
    join geo_city as gc on gc.id = sb.city_id 
where 1=1
    and o.actual_delivery_time >= {{desde}} - interval '2 day'
    and o.actual_delivery_time < {{hasta}} + interval '2 day'
    and o.actual_delivery_time at time zone sb.timezone >= {{desde}}
    and o.actual_delivery_time at time zone sb.timezone < {{hasta}} + interval '1 day'
    and o.currency = 'MXN'
    and o.status = 'DELIVERED' 
    and o.order_kind ='NORMAL'
    and orr.delivery_status = 'DELIVERY_ON_DESTINATION'
