select
    csb.store_id,
    distinct cp.id as catalog_product_id,
    csb.id as branch,
    cpb.id as productbranch_id
from catalog_productbranch cpb
    join catalog_storebranch csb on cpb.branch_id = csb.id
    join catalog_product cp on cpb.product_id = cp.id
where 1=1
    and csb.store_id = {{store_id}}
    and cpb.active
    and csb.active
    and cp.is_active
