select
    sb.store_id,
    p.id as catalog_product_id,
    unnest(barcodes) as barcodes,
    sb.id as branch,
    pb.id as productbranch_id,
    sum(pb.price) as price
from catalog_productbranch pb
    join catalog_storebranch sb on pb.branch_id = sb.id
    join catalog_product p on pb.product_id = p.id
where 1=1
    and sb.store_id = {{store_id}}
    and pb.active
    and sb.active
    and p.is_active
group by 1,2,3,4,5;
